<body>
    <% if (locals.error) { %>
        <div class="alert alert-danger" role="alert" style="padding: 10px 10px 10px; margin-left: 500px; width: 35rem; font-family: 'Montserrat', sans-serif;" margin-bottom: 1rem;">
            Incorrect Password
        </div>
        <% } %>
            <form method="post" class="form-control"
                style="width: 35rem; background-color: #F5F5F5; padding: 10px 10px 10px; margin: 125px 500px 250px 500px;">
                <div class="row g-3 align-items-center">
                    <div class="col-auto">
                        <label for="password" class="col-form-label" style="font-family: 'Montserrat', sans-serif;">Password:</label>
                    </div>
                    <div class="col-auto">
                        <input type="password" id="inputPassword6" style="font-family: 'Montserrat', sans-serif;" class="form-control" name="password" required />
                    </div>
                    <div class="col-auto">
                        <button style="grid-column: span 2; font-family: 'Montserrat', sans-serif;" type="submit" class="btn btn-primary">Download</button>
                    </div>
                    
                </div>
            </form>
</body>

